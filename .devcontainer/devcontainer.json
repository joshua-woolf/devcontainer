{
  "name": "dev",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "DOTNET_VERSION": "9.0",
      "GIT_DELTA_VERSION": "0.18.2",
      "GITUI_VERSION": "0.27.0",
      "ARG HELM_VERSION": "3.18.6",
      "ARG K8S_VERSION": "1.33.2",
      "K9S_VERSION": "0.50.9",
      "KUSTOMIZE_VERSION": "5.7.1",
      "LAZYDOCKER_VERSION": "0.24.1",
      "POWERSHELL_VERSION": "7.5.2",
      "TERRAFORM_VERSION": "1.13.0",
      "TZ": "Africa/Johannesburg",
      "USERNAME": "vscode"
    }
  },
  "mounts": [
    "source=dev-cache-apt-${devcontainerId},target=/var/cache/apt,type=volume",
    "source=dev-cache-apt-lib-${devcontainerId},target=/var/lib/apt,type=volume",
    "source=dev-cache-aws-${devcontainerId},target=/home/vscode/.aws,type=volume",
    "source=dev-cache-azure-${devcontainerId},target=/home/vscode/.azure,type=volume",
    "source=dev-cache-claude-${devcontainerId},target=/home/vscode/.claude,type=volume",
    "source=dev-cache-docker-${devcontainerId},target=/var/lib/docker,type=volume",
    "source=dev-cache-helm-${devcontainerId},target=/home/vscode/.cache/helm,type=volume",
    "source=dev-cache-helm-config-${devcontainerId},target=/home/vscode/.config/helm,type=volume",
    "source=dev-cache-history-${devcontainerId},target=/home/vscode/.history,type=volume",
    "source=dev-cache-kube-${devcontainerId},target=/home/vscode/.kube,type=volume",
    "source=dev-cache-npm-${devcontainerId},target=/home/vscode/.npm,type=volume",
    "source=dev-cache-nuget-${devcontainerId},target=/home/vscode/.nuget,type=volume",
    "source=dev-cache-pip-${devcontainerId},target=/home/vscode/.cache/pip,type=volume",
    "source=dev-cache-pipx-${devcontainerId},target=/home/vscode/.local/share/pipx,type=volume",
    "source=dev-cache-pipx-cache-${devcontainerId},target=/home/vscode/.cache/pipx,type=volume",
    "source=dev-cache-pnpm-${devcontainerId},target=/home/vscode/.local/share/pnpm,type=volume",
    "source=dev-cache-terraform-${devcontainerId},target=/home/vscode/.terraform.d,type=volume",
    "source=dev-cache-yarn-${devcontainerId},target=/home/vscode/.yarn,type=volume"
  ],
  "postCreateCommand": "sudo /usr/local/share/docker-init.sh && bash /usr/local/bin/post-create.sh",
  "privileged": true,
  "remoteUser": "vscode",
  "runArgs": [
    "--cap-add=NET_ADMIN",
    "--cap-add=NET_RAW"
  ],
  "workspaceFolder": "/workspace",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=delegated"
}